<!DOCTYPE html>
<html ng-app="googleMaps">
  <head>
    
    <meta charset="utf-8">
    <title>Google maps with Street View</title>   
    <link rel='stylesheet' href='/css/main.css' />
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
  </head>
  <body ng-controller="mapController as mc" >

    
    <div class="container-fluid" >
      <div class="row chatWrap well well-lg" style="padding-top:0px;" >         
        <div class="col-md-3   " >
            <h3 >Coordinates</h3>
            <div id="data">
            </div>
        </div>
        <div class="col-md- 8 col-md-offset-1   " >
            <div class="row"> 
            <h3>Google map</h3>
            <div id="floating-panel">
                <input type="button" value="Drop pin" onclick="placeMarker()"></input>
            </div>
            <input id="pac-input" class="controls" type="text" placeholder="Search Box">
            <div id="map" style="width:800px;height:200px;position:fixed;">
            </div>
            <!-- </div>
            <div class="row"> -->
              <!-- h3>Street View</h3> -->
              <div id="pano" style="width:800px;height:380px;">
              </div>
            </div>
        </div>
      </div>
    </div>
            

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCWTK0QeemSS_yjMDn8EC8GbsMkPeGO4iA&libraries=places&signed_in=true&callback=initialize">
</script>
<script src="/js/controller.js"></script>
<script src="/js/map.js"></script>
    
  </body>
</html>